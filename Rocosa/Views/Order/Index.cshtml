
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="border p-3 bg-white">
    <div class="d-flex justify-content-between mb-3">
        <div class="p-2">
            <h2 class="text-info">Order List</h2>
        </div>
    </div>
    <table class="table table-striped table-bordered pt-2" style="width:100%;" id="tableData">
        <thead class="theaf-dark">
            <tr class="table-info">
                <th>Id</th>
                <th>Full Name</th>
                <th>Phone Number</th>
                <th>Email</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/order.js"></script>
}

